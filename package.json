{
    "name": "budget-tracker",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && gatsby develop",
        "install": "npm run gatsby",
        "gatsby": "cd client && yarn install",
        "build": "cd client && gatsby build",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run client-build"
    },
    "author": "rlgerma92",
    "license": "WTFPL",
    "devDependencies": {
        "concurrently": "^4.1.0",
        "nodemon": "^1.18.7"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "compression": "^1.7.4",
        "express": "^4.16.3",
        "if-env": "^1.0.4",
        "mongodb": "^3.5.3",
        "mongoose": "^5.2.9",
        "morgan": "^1.9.1",
        "body-parser": "^1.18.3",
        "cors": "^2.8.4",
        "express-jwt": "^5.3.1",
        "jsonwebtoken": "^8.3.0",
        "passport": "^0.4.0",
        "passport-google-token": "^0.1.2"
    }
}
